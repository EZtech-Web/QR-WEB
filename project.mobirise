          "_customHTML": "<section class=\"features\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-fullscreen': fullScreen,\n                    'mbr-parallax-background': bg.parallax}\">\n    \n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"2\"> \n        \n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubTitle\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n\n        <header>Badges</header>\n        <select title=\"Badges\" name=\"badgeAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <input type=\"color\" name=\"cBadge1\" title=\"Badge 1\" value=\"#dc514e\">\n        <input type=\"color\" name=\"cBadge2\" title=\"Badge 2\" value=\"#12ca60\" condition=\"badgeAmount > 1\">\n        <input type=\"color\" name=\"cBadge3\" title=\"Badge 3\" value=\"#53a8ae\" condition=\"badgeAmount > 2\">\n        <input type=\"color\" name=\"cBadge4\" title=\"Badge 4\" value=\"#635aa6\" condition=\"badgeAmount > 3\">\n        <input type=\"color\" name=\"cBadgeBG\" title=\"Badge Background\" value=\"#003e1b\">\n        <input type=\"range\" title=\"Size\" name=\"badgeWidth\" min=\"100\" max=\"250\" step=\"1\" value=\"200\">\n        <input type=\"range\" title=\"Spacing\" name=\"badgeMargin\" min=\"0\" max=\"50\" step=\"1\" value=\"20\">\n\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/mbr-1.jpg\" selected parallax>\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row justify-content-md-center\">\n            <div class=\"col-md-10 align-center\">\n                <h1 class=\"mbr-section-title mbr-bold pb-3 mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\"><b>Liên Hệ Hỗ Trợ Trực Tiếp</b></h1>\n                <h3 class=\"mbr-section-subtitle mbr-light pb-3 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showSubTitle\">Our Certification Path</h3>\n                <p class=\"mbr-text pb-3 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\"><b>Trong trường hợp vì lý do kỹ thuật hoặc tất cả tư vấn viên &amp; cộng tác viên của chúng tôi đang không ở bàn làm việc, vui lòng liên hệ trực tiếp theo danh sách di động sau</b></p>\n            </div>\n            <div class=\"p-3 pt-4 col-md-12 align-center\">\n              <div class=\"badge cBadge1\">\n                <svg xmlns=\"http://www.w3.org/2000/svg\" version=\"1.1\" x=\"0px\" y=\"0px\" viewBox=\"0 0 216 232\">\n                  <path fill=\"{{cBadgeBG}}\" d=\"M207,0C171.827,0.001,43.875,0.004,9.003,0c-5.619-0.001-9,3.514-9,9c0,28.23-0.006,151.375,0,169    c0.005,13.875,94.499,54,107.999,54S216,191.57,216,178V9C216,3.298,212.732,0,207,0z\"></path>\n                </svg>\n                <p class=\"title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title\">Tư Vấn Dịch Vụ<br><span style=\"font-weight: normal;\">Hotline 24/7</span></p>\n                <p class=\"subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".subtitle\"><a href=\"tel:0888100024\" class=\"text-white text-primary\"><b>088 8100 024</b></a></p>\n                <p class=\"stars mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".stars\">Di Động / Zalo</p>\n              </div>\n\n              <div class=\"badge cBadge2\" mbr-if=\"badgeAmount > 1\">\n                <svg xmlns=\"http://www.w3.org/2000/svg\" version=\"1.1\" x=\"0px\" y=\"0px\" viewBox=\"0 0 216 232\">\n                  <path fill=\"{{cBadgeBG}}\" d=\"M207,0C171.827,0.001,43.875,0.004,9.003,0c-5.619-0.001-9,3.514-9,9c0,28.23-0.006,151.375,0,169    c0.005,13.875,94.499,54,107.999,54S216,191.57,216,178V9C216,3.298,212.732,0,207,0z\"></path>\n                </svg>\n                <p class=\"title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title\">Hỗ Trợ Kỹ Thuật<br><span style=\"font-weight: normal;\">Mr. Hữu Tính</span></p>\n                <p class=\"subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".subtitle\"><a href=\"tel:0906424185\" class=\"text-white\"><b>0906 424 185</b></a></p>\n                <p class=\"stars mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".stars\">Zalo / Telegram</p>\n              </div>\n\n              <div class=\"badge cBadge3\" mbr-if=\"badgeAmount > 2\">\n                <svg xmlns=\"http://www.w3.org/2000/svg\" version=\"1.1\" x=\"0px\" y=\"0px\" viewBox=\"0 0 216 232\">\n                  <path fill=\"{{cBadgeBG}}\" d=\"M207,0C171.827,0.001,43.875,0.004,9.003,0c-5.619-0.001-9,3.514-9,9c0,28.23-0.006,151.375,0,169    c0.005,13.875,94.499,54,107.999,54S216,191.57,216,178V9C216,3.298,212.732,0,207,0z\"></path>\n                </svg>\n                <p class=\"title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title\">Hỗ Trợ Đào Tạo<br><span style=\"font-weight: normal;\">Ms. Huyền Mai</span></p>\n                <p class=\"subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".subtitle\"><a href=\"tel:0368500247\" class=\"text-white text-primary\"><b>036 8500 247</b></a></p>\n                <p class=\"stars mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".stars\">(09:00 - 21:00)</p>\n              </div>\n\n              <div class=\"badge cBadge4\" mbr-if=\"badgeAmount > 3\">\n                <svg xmlns=\"http://www.w3.org/2000/svg\" version=\"1.1\" x=\"0px\" y=\"0px\" viewBox=\"0 0 216 232\">\n                  <path fill=\"{{cBadgeBG}}\" d=\"M207,0C171.827,0.001,43.875,0.004,9.003,0c-5.619-0.001-9,3.514-9,9c0,28.23-0.006,151.375,0,169    c0.005,13.875,94.499,54,107.999,54S216,191.57,216,178V9C216,3.298,212.732,0,207,0z\"></path>\n                </svg>\n                <p class=\"title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title\">Chuyên Viên Web<br><span style=\"font-weight: normal;\">Mr. Tiến Huy</span></p>\n                <p class=\"subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".subtitle\"><a href=\"tel:0787700247\" class=\"text-white\"><b>078 7700 247</b></a></p>\n                <p class=\"stars mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".stars\">Di Động / Zalo</p>\n              </div>\n            </div>\n        </div>\n    </div>\n</section>",
